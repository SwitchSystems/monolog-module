{
    "name": "neeckeloo/monolog-module",
    "description": "Monolog integration into Zend Framework 2",
    "type": "library",
    "keywords": [
        "zf2", "monolog", "logger", "log"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/MonologModule",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "monolog/monolog": "~1.11",
        "zendframework/zend-servicemanager": "~2.3"
    },
    "require-dev": {
        "graylog2/gelf-php": "~1.1",
        "phpunit/phpunit": "~4.3",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zendframework": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "MonologModule\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MonologModuleTest\\": "tests/"
        }
    }
}
